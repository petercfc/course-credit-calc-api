type Student {
  id: ID! @id
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
  name: String
  coursesPassed: [Course]
  enrolledDegree:Degree
}

type Subject {
  id: ID! @id
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
  name: String!
  description: String!
}

type Degree {
  id: ID! @id
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
  name: String!
  description: String!
  requiredCredits: Int!
  requiredCourses: [Course]
  department: Department
}

type Department {
  id: ID! @id
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
  name: String!
  description: String!
}

type Course {
  id: ID! @id
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
  name: String!
  description: String!
  number:String!
  level: Int!
  credits: Int!
  subject: Subject
  degree: Degree
  department: Department
  prerequisite:Course
}